{% if item_type == 'card' %}
  {% include "@molecules/card/card.twig" with {
    card__extra_classes: item.card__extra_classes,
    card__blockname: item.card__blockname,
    card__modifiers: item.card__modifiers,
    card__image__src: item.card__image__src,
    card__image__alt: item.card__image__alt,
    card__date_month: item.card__date_month,
    card__date_day: item.card__date_day,
    card__date: item.card__date,
    card__host: item.card__host,
    card__heading: item.card__heading,
    card__subheading: item.card__subheading,
    card__body: item.card__body,
    card__link__url: item.card__link__url,
    card__link__text: item.card__link__text,
    card__image__output_image_tag: item.card__image__output_image_tag,
  } %}
{% elseif item_type == 'cta' %}
  {% include "@molecules/cta/cta-icon.twig" with {
    cta_icon_extra_classes: ['grid__item-four'],
    cta_icon_modifiers: item.cta_icon_modifiers,
    cta_icon_icon: item.cta_icon_icon,
    cta_icon_link: '#',
    cta_icon_heading: item.cta_icon_heading,
    cta_icon_body: item.cta_icon_body,
  } %}
{% else %}
  <div class="grid__item grid__item--example"></div>
{% endif %}
